Program Hello; // ubah menjadi Program APCI-NIM
            Uses Crt;
            Begin
                ClrScr;
                Writeln(‘Hello’);//untuk mengeluarkan string ‘Hello’ di layar
                Writeln(‘1301154233’); //Ubah untuk menampilkan NIM Anda
                Writeln(‘Satriahutomo’); //Ubah untuk menampilkan Nama Anda
                Writeln(‘IF3909’); //Ubah untuk menampilkan Kelas Anda
                Writeln(‘Graham’); //Ubah untuk menampilkan Nama Asprak Anda
                Writeln(‘Kukuh’); //Ubah untuk menampilkan NIM teman anda
                Writeln(‘Modul 10’); //Ubah untuk menampilkan string ‘Modul 6
            End.